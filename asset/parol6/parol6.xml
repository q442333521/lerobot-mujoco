<mujoco model="parol6">
  <compiler angle="radian" autolimits="true"/>

  <option integrator="implicitfast"/>

  <!-- Default -->
  <default>
    <default class="PAROL6">
      <joint armature="0.1" damping="100.0"/>
      <position kp="3000" forcerange="-1500 1500"/>
    </default>
    <default class="gripper">
      <joint armature="0.1" damping="10.0"/>
      <position kp="1000" forcerange="-100 100"/>
    </default>
  </default>

  <asset>
    <!-- PAROL6机械臂STL网格 -->
    <mesh name="base_link" file="parol6/meshes/base_link.STL" scale="0.001 0.001 0.001"/>
    <mesh name="L1" file="parol6/meshes/L1.STL" scale="0.001 0.001 0.001"/>
    <mesh name="L2" file="parol6/meshes/L2.STL" scale="0.001 0.001 0.001"/>
    <mesh name="L3" file="parol6/meshes/L3.STL" scale="0.001 0.001 0.001"/>
    <mesh name="L4" file="parol6/meshes/L4.STL" scale="0.001 0.001 0.001"/>
    <mesh name="L5" file="parol6/meshes/L5.STL" scale="0.001 0.001 0.001"/>
    <mesh name="L6" file="parol6/meshes/L6.STL" scale="0.001 0.001 0.001"/>

    <!-- 夹爪STL网格 -->
    <mesh name="gripper_base" file="parol6/meshes/gripper_base.stl" scale="0.001 0.001 0.001"/>
    <mesh name="gripper_jaw" file="parol6/meshes/gripper_jaw.stl" scale="0.001 0.001 0.001"/>
  </asset>

  <worldbody>
    <!-- PAROL6机械臂基座 -->
    <body name="base_link" pos="0 0 0.8715">
      <inertial pos="-0.0275 -0.0014 0.0312" mass="0.8127" diaginertia="0.001107 0.001066 0.001479"/>
      <geom type="mesh" mesh="base_link" rgba="0.95 0.95 0.95 1"/>

      <!-- 关节1: L1 -->
      <body name="L1" pos="0.0000 0.0000 0.0000" quat="1.0000 0.0000 0.0000 0.0000">
        <inertial pos="0.0052 0.0290 0.0993" mass="0.6448" diaginertia="0.001094 0.000570 0.001295"/>
        <joint name="L1" type="hinge" axis="0.0 0.0 1.0" range="-1.70 1.70" class="PAROL6"/>
        <geom type="mesh" mesh="L1" rgba="0.3 0.3 0.8 1"/>

        <!-- 关节2: L2 -->
        <body name="L2" pos="0.0234 0.0000 0.1105" quat="0.7071 -0.7071 0.0000 0.0000">
          <inertial pos="-0.0094 -0.0784 0.0369" mass="0.5123" diaginertia="0.001234 0.000205 0.001322"/>
          <joint name="L2" type="hinge" axis="0.0 0.0 1.0" range="-0.98 1.00" class="PAROL6"/>
          <geom type="mesh" mesh="L2" rgba="0.8 0.3 0.3 1"/>

          <!-- 关节3: L3 -->
          <body name="L3" pos="0.0000 -0.1800 0.0000" quat="-0.0000 0.7071 -0.7071 0.0000">
            <inertial pos="0.0156 -0.0191 -0.0015" mass="0.5510" diaginertia="0.000578 0.000567 0.000816"/>
            <joint name="L3" type="hinge" axis="0.0 0.0 -1.0" range="-2.00 1.30" class="PAROL6"/>
            <geom type="mesh" mesh="L3" rgba="0.3 0.8 0.3 1"/>

            <!-- 关节4: L4 -->
            <body name="L4" pos="0.1485 0.0000 0.0000" quat="-0.7071 0.0000 -0.7071 0.0000">
              <inertial pos="0.0000 -0.0028 -0.0573" mass="0.2987" diaginertia="0.000376 0.000214 0.000261"/>
              <joint name="L4" type="hinge" axis="0.0 0.0 -1.0" range="-2.00 2.00" class="PAROL6"/>
              <geom type="mesh" mesh="L4" rgba="0.8 0.8 0.3 1"/>

              <!-- 关节5: L5 -->
              <body name="L5" pos="0.0000 0.0000 -0.1550" quat="0.7071 -0.7071 0.0000 0.0000">
                <inertial pos="0.0041 -0.0211 -0.0000" mass="0.2145" diaginertia="0.000119 0.000059 0.000117"/>
                <joint name="L5" type="hinge" axis="0.0 0.0 -1.0" range="-2.10 2.10" class="PAROL6"/>
                <geom type="mesh" mesh="L5" rgba="0.8 0.3 0.8 1"/>

                <!-- 关节6: L6 -->
                <body name="L6" pos="0.0000 -0.0585 0.0000" quat="0.7071 0.0000 -0.7071 0.0000">
                  <inertial pos="0.0000 0.0102 -0.0001" mass="0.1037" diaginertia="0.000046 0.000024 0.000042"/>
                  <joint name="L6" type="hinge" axis="0.0 0.0 -1.0" limited="false" class="PAROL6"/>
                  <geom type="mesh" mesh="L6" rgba="0.3 0.8 0.8 1"/>

                  <!-- 末端执行器site和摄像头 -->
                  <site name="end_effector" pos="0 0 -0.13" size="0.01"/>

                  <!-- 夹爪视角摄像头 -->
                  <camera name="gripper_cam" pos="0 -0.1 -0.05" xyaxes="0 0 1 -1 0 0" mode="fixed" fovy="60"/>

                  <!-- 夹爪（真实STL模型） -->
                  <body name="gripper_base" pos="0 0 -0.02">
                    <inertial pos="0 0 0" mass="0.1" diaginertia="0.0001 0.0001 0.0001"/>
                    <geom type="mesh" mesh="gripper_base" rgba="0.5 0.5 0.5 1"/>

                    <!-- 左侧夹爪指 -->
                    <body name="gripper_left" pos="0 0.01 -0.11">
                      <inertial pos="0 0.02 0" mass="0.05" diaginertia="0.00005 0.00005 0.00005"/>
                      <joint name="rh_l1" type="slide" axis="0 1 0" range="0 0.03" class="gripper"/>
                      <geom type="mesh" mesh="gripper_jaw" rgba="0.4 0.4 0.4 1"/>
                    </body>

                    <!-- 右侧夹爪指 -->
                    <body name="gripper_right" pos="0 -0.01 -0.11" euler="0 0 3.14159">
                      <inertial pos="0 0.02 0" mass="0.05" diaginertia="0.00005 0.00005 0.00005"/>
                      <joint name="rh_r1" type="slide" axis="0 1 0" range="0 0.03" class="gripper"/>
                      <geom type="mesh" mesh="gripper_jaw" rgba="0.4 0.4 0.4 1"/>
                    </body>
                  </body>

                  <!-- TCP链接 -->
                  <body name="tcp_link" pos="0 0 -0.22">
                    <geom type="capsule" fromto="0 0 -0.5 0 0 0.5" size="0.01" rgba="1.0 0.2 0.2 0.0" contype="0" conaffinity="0"/>
                  </body>
                </body>
              </body>
            </body>
          </body>
        </body>
      </body>
    </body>
  </worldbody>

  <actuator>
    <!-- 机械臂关节执行器 -->
    <position name="L1_motor" joint="L1" ctrlrange="-1.70 1.70" class="PAROL6"/>
    <position name="L2_motor" joint="L2" ctrlrange="-0.98 1.00" class="PAROL6"/>
    <position name="L3_motor" joint="L3" ctrlrange="-2.00 1.30" class="PAROL6"/>
    <position name="L4_motor" joint="L4" ctrlrange="-2.00 2.00" class="PAROL6"/>
    <position name="L5_motor" joint="L5" ctrlrange="-2.10 2.10" class="PAROL6"/>
    <position name="L6_motor" joint="L6" ctrlrange="-3.14 3.14" class="PAROL6"/>

    <!-- 夹爪执行器 -->
    <position name="rh_l1_motor" joint="rh_l1" ctrlrange="0 0.03" class="gripper"/>
    <position name="rh_r1_motor" joint="rh_r1" ctrlrange="0 0.03" class="gripper"/>
  </actuator>

  <sensor>
    <!-- 机械臂关节传感器 -->
    <jointpos name="L1_pos" joint="L1"/>
    <jointpos name="L2_pos" joint="L2"/>
    <jointpos name="L3_pos" joint="L3"/>
    <jointpos name="L4_pos" joint="L4"/>
    <jointpos name="L5_pos" joint="L5"/>
    <jointpos name="L6_pos" joint="L6"/>
    <jointvel name="L1_vel" joint="L1"/>
    <jointvel name="L2_vel" joint="L2"/>
    <jointvel name="L3_vel" joint="L3"/>
    <jointvel name="L4_vel" joint="L4"/>
    <jointvel name="L5_vel" joint="L5"/>
    <jointvel name="L6_vel" joint="L6"/>

    <!-- 夹爪传感器 -->
    <jointpos name="rh_l1_pos" joint="rh_l1"/>
    <jointpos name="rh_r1_pos" joint="rh_r1"/>
  </sensor>

</mujoco>
